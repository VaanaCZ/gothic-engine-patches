// HumanS_Carry fixes for Union
// created by Vaana

#engine [G1]
    #patch [Focus fix]
        // mob
        #assembler [0x006910CF]
			; Check right hand
            mov     ecx, edi
            call    0x00697750  ; oCNpc::GetRightHand
            cmp     eax, esi
            jz      0x006911E5
			
			; Check left hand
            mov     ecx, edi
            call    0x006977E0  ; oCNpc::GetLeftHand
            cmp     eax, esi
            jz      0x006911E5
            orgcode
        #/assembler
		
        // nsc
        #assembler [0x00691015]
			; Check right hand
            mov     ecx, edi
            call    0x00697750  ; oCNpc::GetRightHand
            cmp     eax, esi
            jz      0x006911E5
			
			; Check left hand
            mov     ecx, edi
            call    0x006977E0  ; oCNpc::GetLeftHand
            cmp     eax, esi
            jz      0x006911E5
            orgcode
        #/assembler
    #/patch
#/engine